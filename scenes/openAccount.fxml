<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minWidth="60.0" prefHeight="98.0" prefWidth="547.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fitz.openAccountController">
   <children>
      <ComboBox fx:id="cbox_acc" maxWidth="1.7976931348623157E308" onAction="#getAccSelected" prefWidth="150.0" promptText="Please select account">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </ComboBox>
      <HBox prefHeight="17.0" prefWidth="527.0">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_open" mnemonicParsing="false" onAction="#openAccount" style="-fx-background-color: linear-gradient(to bottom, #639ddb,#447cd6);" text="Open" textFill="#f8f6f6">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
   </padding>
</VBox>
